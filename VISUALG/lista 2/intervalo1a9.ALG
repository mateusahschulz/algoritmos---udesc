algoritmo "intervalo 1 a 9"
var
   valor1: inteiro
inicio
   escreva("Digite o valor 1: ")
   leia(valor1)
   escolha valor1
      caso 1 ate 9
         escreva("O valor está na faixa permitida")
      outrocaso
         escreva("O valor está fora da faixa permitida")
      fimescolha
fimalgoritmo